type News {
  id: ID!
  title: String!
  text: String!
  published: Boolean!
  createdById: ID!
  approvedById: ID!
  createdAt: String!
  createdBy: User!
  approvedBy: User!
  comments: [Comment!]
}

input NewsInput {
  title: String!
  text: String!
  createdById: ID!
}

extend type Query {
  news(newsId: String!): News!
  allNews: [News!]
}

extend type Mutation {
  createNews(input: NewsInput!): News!
}
